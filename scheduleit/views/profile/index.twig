{% extends 'layouts/logged_in.twig' %}

{% block page_content %}

<h1 class="h2 mb-4">
  {{title}}
</h1>
<div class="row">
  <div class="col-md-6">
    <div class="row">
      <div class="col-6 col-md-8 col-lg-6">
        <h4 class="h6">
          Name
        </h4>
        <p>
          {{user_firstname}} {{user_lastname}}
        </p>
        <h4 class="h6">
          Email
        </h4>
        <p>
          {{user_email}}
        </p>
      </div>
    </div>
  </div>
</div>

{% endblock %}
