{% extends 'layouts/logged_in.twig' %}

{% block page_content %}

<h1 class="h2 mb-4">
  {{title}}
</h1>
<div class="mb-4">
  <div class="d-inline-block mr-3">
    <span class="d-inline-block mr-2 px-2 py-1 calendar-meeting-mine"></span>
    Meetings Created By Me
  </div>
  <div class="d-inline-block ">
    <span class="d-inline-block mr-2 px-2 py-1 calendar-meeting-other"></span>
    Meetings Created By Others
  </div>
</div>
<div
  class="mb-5"
  data-calendar-events="{{meetings_json}}"
  data-site-url="{{site_url}}"
  data-user-id="{{user_id}}"
  id="calendar"
>
</div>

{% endblock %}
